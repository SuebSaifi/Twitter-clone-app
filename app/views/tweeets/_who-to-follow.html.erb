<div class="column">
    <nav class="panel">
        <p class="panel-heading">Who To Follow</p>
    </nav>

<span>Add friends<span>
<% @friendships.each do |user| %>
<%if !current_user.friends.include?(user)%>
<%if current_user!=user%>
  <div class="user">
    <p>
      <strong><%=user.username %></strong>
      <%= link_to "Add Friend", friendships_path(friend_id: user), :method => :post ,class:"icon"%>     
      </p>
  </div>
  <%end%>
  <%end%> 
  <%end%>
  